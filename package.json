{
  "name": "ikono",
  "version": "1.9.0",
  "description": "A large collection of high quality SVG icons with an MIT license",
  "licence": "MIT",
  "scripts": {
    "optimize": "ts-node ./scripts/build.ts",
    "build:react_library": "ts-node ./scripts/build.ts && ts-node ./scripts/build_react.ts && cd react && npm run build",
    "build:web": "ts-node ./scripts/generate_pngs.ts && ts-node ./scripts/build.ts",
    "version": "ts-node ./scripts/bump_all.ts && ts-node ./scripts/release_current_version.ts",
    "fetchIcons": "ts-node ./scripts/fetch_icons.ts",
    "test": "ts-node ./test/validate_meta.ts"
  },
  "author": "wkelly1",
  "files": [
    "src/optimised"
  ],
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "sharp": "^0.31.3",
    "svg-to-jsx": "^1.0.3",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "icons",
    "svg",
    "library"
  ],
  "dependencies": {
    "@iconify/tools": "^3.0.5"
  }
}
